Author = {Seong, KS and Kyung, CM},
Book-Group-Author = {IEEE},
Title = {CBLO: A clustering based linear ordering for netlist partitioning},
Booktitle = {PROCEEDINGS OF THE ASP-DAC `97 - ASIA AND SOUTH PACIFIC DESIGN
   AUTOMATION CONFERENCE 1997},
Year = {1996},
Pages = {43-48},
Note = {Asia and South Pacific Design Automation Conference 1997 (ASP-DAC 97),
