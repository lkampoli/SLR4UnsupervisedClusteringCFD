@article{ WOS:000626422300002,
Author = {Yousefi, Hadi and Gholamian, S. Asghar and Zakariazadeh, Alireza},
Title = {Voltage control approach based on PCPM distributed algorithm in the
   presence of high PV penetration: a stochastic modeling},
Journal = {ELECTRICAL ENGINEERING},
Year = {2021},
Volume = {103},
Number = {6},
Pages = {2561-2572},
Month = {DEC},
Abstract = {The uncertainty of solar generation is a challenging issue for the
   operation and control of distribution systems. Also, because of the
   large number of control and operation variables in advanced distribution
   networks, the centralized optimization technique requires high
   computational capabilities. In this paper, a distributed voltage
   regulation approach in a distribution system based on the predictor
   corrector proximal multiplier (PCPM) algorithm has been presented. This
   paper focuses on the voltage rise in distribution systems with high
   penetration of photovoltaics. The proposed method uses a clustering
   approach to divide the network into partitions based on the coupling
   degrees among different nodes. The optimal reactive power control
   strategy is conducted in each partition and integrated using PCPM. The
   proposed method is tested on 69 and 123 nodes IEEE distribution test
   systems. The results confirmed that the proposed method achieves the
   optimal solution with the same accuracy as the centralized method but in
   a much shorter computation time.},
