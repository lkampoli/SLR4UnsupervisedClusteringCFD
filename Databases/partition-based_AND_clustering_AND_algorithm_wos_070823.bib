@article{ WOS:000582721200077,
Author = {Castiblanco, Fabian and Franco, Camilo and Tinguaro Rodriguez, J. and
   Montero, Javier},
Title = {Evaluation of the quality and relevance of a fuzzy partition},
Journal = {JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS},
Year = {2020},
Volume = {39},
Number = {3},
Pages = {4211-4226},
Abstract = {This paper proposes a couple of criteria for evaluating the quality and
   relevance of a fuzzy partition. These criteria are established from a
   fuzzy classification system and its recursive De Morgan triplet. We
   propose a comparison process between the classes of a fuzzy partition,
   based on a translation invariant similarity relation. Therefore a
   classification process is carried out with the equivalence relations
   determined by the similarity relation. Such a relation is built on the
   commutative group structure formed by the elements of the fuzzy
   classification system. Our approach is illustrated through an example on
   image analysis by the fuzzy c-means algorithm.},
