@inproceedings{ WOS:000392812600325,
Author = {Abolfazli, Zahra and Mahdavi, Mehdi},
Book-Group-Author = {IEEE},
Title = {A Homogeneous Wireless Sensor Network Routing Algorithm: An Energy Aware
   Cluster Based Approach},
Booktitle = {2014 22ND IRANIAN CONFERENCE ON ELECTRICAL ENGINEERING (ICEE)},
Series = {Iranian Conference on Electrical Engineering},
Year = {2014},
Pages = {1717-1722},
Note = {22nd Iranian Conference on Electrical Engineering (ICEE), Shahid
   Beheshti Univ, Tehran, IRAN, MAY 20-22, 2014},
Abstract = {An energy efficient routing algorithm is the major concern in wireless
   sensor network. Routing protocols can be categorized into four groups
   which are including quality of service (QoS) routing protocols,
   location-based routing protocols data-centric routing protocols and
   hierarchical -based routing protocols. For those applications in which
   the sensors are engaged in environment discovery, the hierarchical-based
   routing protocols are more efficient. In the other way, clustering
   techniques have been shown to be useful in both energy and scalability
   points of view. Hence, in this paper an energy aware routing algorithm
   based on clustering techniques is proposed. The proposed algorithm is
   suitable for the multi-hop large scale networks. Our main proposal is
   apply the network leveling techniques by which the efficiency of
   clustering based on HEED algorithm is improved. We compare our proposed
   algorithm with the known algorithms such as BMR and EELBCRP. In
   comparison with the existing algorithms, the simulation results of our
   proposed algorithm present the enhancement of the network lifetime
   significantly. Moreover, our proposed algorithm prevents the network
   from partitioning based on the energy consumption balance that it
   produces in the network.},
Publisher = {IEEE},
Address = {345 E 47TH ST, NEW YORK, NY 10017 USA},
Type = {Proceedings Paper},
