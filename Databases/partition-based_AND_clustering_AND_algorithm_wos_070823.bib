@article{ WOS:001006217100001,
Author = {Sun, Wenwen and He, Guoqing},
Title = {Cluster Partition-Based Voltage Control Combined Day-Ahead Scheduling
   and Real-Time Control for Distribution Networks},
Journal = {ENERGIES},
Year = {2023},
Volume = {16},
Number = {11},
Month = {MAY 27},
Abstract = {Considering the possible overvoltage caused by high-penetration
   photovoltaics (PVs) connected to the distribution networks (DNs), a
   cluster partition-based voltage control combined day-ahead scheduling
   and real-time control for distribution networks is proposed. Firstly, a
   community detection algorithm utilizing a coupling quality function is
   introduced to divide the PVs into clusters. Based on the cluster
   partition, day-ahead scheduling (DAS) is proposed with the objective of
   minimizing the operating costs of PVs, as well as the on-load tap
   changer (OLTC). In the real-time control, a second-order cone
   programming (SOCP) model-based real-time voltage control (RTVC) strategy
   is drawn up in each cluster to regulate the PV inverters, and this
   strategy can correct the day-ahead scheduling by modifications. The
   proposed strategy realizes the combination of day-ahead scheduling and
   real-time voltage control, and the optimization of voltage control can
   be greatly simplified. Finally, the proposed method is applied to a
   practical 10 kV feeder to verify its effectiveness.},
