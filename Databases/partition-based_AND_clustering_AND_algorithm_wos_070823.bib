
@article{ WOS:000372047400005,
Author = {Jin, Ran and Kou, Chunhai and Liu, Ruijuan and Guo, Tao},
Title = {A COMMON FRAMEWORK OF PARTITION-BASED CLUSTERING FOR LARGE SCALE DATASET
   USING SAMPLING AND ITS MapReduce IMPLEMENTATION},
