Title = {Urban Power Grid Electromagnetic Loop Breaking Method Based on Fast
   Network Partition Algorithm},
Booktitle = {2018 CHINA INTERNATIONAL CONFERENCE ON ELECTRICITY DISTRIBUTION (CICED)},
