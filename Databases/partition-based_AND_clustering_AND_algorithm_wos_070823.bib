@article{ WOS:000417722100003,
Author = {Sathiya, B. and Geetha, T. V. and Saruladha, K.},
Title = {PSOM2-partitioning-based scalable ontology matching using MapReduce},
Journal = {SADHANA-ACADEMY PROCEEDINGS IN ENGINEERING SCIENCES},
Year = {2017},
Volume = {42},
