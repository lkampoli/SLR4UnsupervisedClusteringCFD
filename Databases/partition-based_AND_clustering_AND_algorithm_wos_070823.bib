Title = {Elephant Search Algorithm on Data Clustering},
Booktitle = {2016 12TH INTERNATIONAL CONFERENCE ON NATURAL COMPUTATION, FUZZY SYSTEMS
   AND KNOWLEDGE DISCOVERY (ICNC-FSKD)},
Year = {2016},
Pages = {787-793},
