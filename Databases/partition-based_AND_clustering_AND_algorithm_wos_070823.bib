@inproceedings{ WOS:000392747900011,
Author = {Ails, C. and Boehm, J. and Liu, K.},
Editor = {Halounova, L and Li, S and Safar, V and Tomkova, M and Rapant, P and Brazdil, K and Shi, W and Anton, F and Liu, Y and Stein, A and Cheng, T and Pettit, C and Li, QQ and Sester, M and Mostafavi, MA and Madden, M and Tong, X and Brovelli, MA and HaeKyong, K and Kawashima, H and Coltekin, A},
Title = {PARALLEL PROCESSING OF BIG POINT CLOUDS USING Z-ORDER-BASED PARTITIONING},
