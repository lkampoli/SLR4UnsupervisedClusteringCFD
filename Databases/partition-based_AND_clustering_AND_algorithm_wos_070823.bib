@article{ WOS:000483692100010,
Author = {Chen, Fangshu and Qi, Jianzhong and Lin, Huaizhong and Gao, Yunjun and
   Lu, Dongming},
Title = {GOAL: a clustering-based method for the group optimal location problem},
Journal = {KNOWLEDGE AND INFORMATION SYSTEMS},
Year = {2019},
Volume = {61},
Number = {2},
Pages = {873-903},
Month = {NOV},
Abstract = {Optimal location problems are important problems and are particularly
   useful for strategic planning of resources. However, existing studies
   mainly focus on computing one or k optimal locations. We study the Group
   OptimAl Location (GOAL) problem, which computes a minimum set of
   locations such that establishing facilities at these locations
   guarantees that every facility user can access at least one facility
   within a given distance r is an element of R. We propose two algorithms,
   GOAL-Greedy and GOAL-DP, to first solve the problem in the Euclidean
   space. These two algorithms are supported by a clustering-based method
   to compute an initial solution of the problem, which yields an upper
   bound of the number of locations needed to solve the problem. We propose
   a grid partitioning-based strategy to refine the initial solution and
   obtain the final solution. We further extend our algorithms to road
   networks. We perform extensive experiments on the proposed algorithms.
   The results show that the proposed algorithms can solve the problem
   effectively and efficiently in both Euclidean spaces and road networks.},
