@inproceedings{ WOS:000844554800097,
Author = {Di Nardo, A. and Di Natale, M. and Giudicianni, C. and Greco, R. and
   Santonastaso, G. F.},
Editor = {Cherifi, C and Cherifi, H and Karsai, M and Musolesi, M},
Title = {Water Distribution Network Clustering: Graph Partitioning or Spectral
   Algorithms?},
Booktitle = {COMPLEX NETWORKS \& THEIR APPLICATIONS VI},
Series = {Studies in Computational Intelligence},
Year = {2018},
Volume = {689},
Pages = {1197-1209},
Note = {6th International Conference on Complex Networks and Their Applications
   (COMPLEX NETWORKS), Univ Lyon 2, Lyon, FRANCE, NOV 29-DEC 01, 2017},
Abstract = {Water Network Partitioning (WNP) is among the most attractive and
   studied strategies for the improvement of the Water Distribution Network
   (WDN) management. The proper definition of subregions (called clusters
   or districts) with high link density between nodes in the same group,
   and a relatively low link density between nodes in different groups, is
   a crucial aspect for the partitioning of a water system. If on one hand
   the definition of these monitored sub-areas, called District Metered
   Areas (DMAs), allows simplifying the water balance, the pressure
   control, the water leaks identification and the water quality
   protection, on the other hand it may worsen the hydraulic performance
