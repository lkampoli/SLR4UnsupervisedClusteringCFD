@article{ WOS:000666417100001,
Author = {Zhu, Lilu and Su, Xiaolu and Hu, Yanfeng and Tai, Xianqing and Fu, Kun},
Title = {A Spatio-Temporal Local Association Query Algorithm for Multi-Source
   Remote Sensing Big Data},
