@article{ WOS:000225367500002,
Author = {Tang, K and Liu, YJ},
Title = {An optimization algorithm for free-form surface partitioning based on
   weighted Gaussian image},
Journal = {GRAPHICAL MODELS},
Year = {2005},
Volume = {67},
Number = {1},
Pages = {17-42},
Month = {JAN},
Abstract = {Partitioning free-form surfaces into sub-patches and finding optimal
   representative normal for each patch to maximize a global objective
   function is an important two-level operation in diverse industrial
   applications. In this paper, by solving a maximum hemispherical
   partitioning problem raised from a weighted Gaussian image, an
   optimization algorithm is proposed to partition a free-form surface into
   two sub-patches and simultaneously report the optimal representative
   normals. By discretizing the free-form surface with W sample points and
   clustering normals on the surface with m distinct sample normals, the
   proposed algorithm is designed, in general, with O(m(2)W(2)) time
   complexity and O(V) space complexity, and in particular, if the surface
   is convex, in O(m(2)logm) time complexity. Case studies with four
   representative examples are presented and a real world application is
   exploited to demonstrate the effectiveness and usefulness of the
   proposed algorithm. (C) 2004 Elsevier Inc. All rights reserved.},
