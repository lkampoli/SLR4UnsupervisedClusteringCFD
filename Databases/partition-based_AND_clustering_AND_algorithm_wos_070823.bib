@inproceedings{ WOS:000427628300031,
Author = {Shukla, Aparna and Kanungo, Suvendu},
Editor = {Astya, PN and Swaroop, A and Sharma, V and Singh, M and Gupta, K},
Title = {A NOVEL CLUSTERING FRAMEWORK USING FARTHEST NEIGHBOUR APPROACH},
Booktitle = {2017 IEEE INTERNATIONAL CONFERENCE ON COMPUTING, COMMUNICATION AND
   AUTOMATION (ICCCA)},
Year = {2017},
Pages = {164-169},
Note = {IEEE International Conference on Computing, Communication and Automation
   (ICCCA), Noida, INDIA, MAY 05-06, 2017},
Organization = {IEEE; IEEE UP Sect; GALGOTIAS UNIV, Sch Comp Sci \& Engn},
Abstract = {In this digital world, we are facing the flood ofdata, but depriving for
   knowledge. The eminent need of mining isuseful to extract the hidden
   pattern from the wide availability of vastamount of data. Clustering is
   one such useful mining tool to handle this unfavorable situation by
   carrying out crucial steps refers ascluster analysis. It is the process
   of a grouping of patterns intoclusters based on similarity. Partition
   based clustering algorithms arewidely accepted for much diverse
   application such as patternanalysis, image segmentation, identification
   system. Among thedifferent variations of the partition based clustering,
   due to itsmonotony and ease of implementation K-means algorithm gained
