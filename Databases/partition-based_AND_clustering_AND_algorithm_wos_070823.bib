Title = {VPN-nonVPN Traffic Classification Using Deep Reinforced Naive Bayes and
   Fuzzy K-means Clustering},
Booktitle = {2021 IEEE 41ST INTERNATIONAL CONFERENCE ON DISTRIBUTED COMPUTING SYSTEMS
   WORKSHOPS (ICDCSW 2021)},
Series = {IEEE International Conference on Distributed Computing Systems Workshops},
